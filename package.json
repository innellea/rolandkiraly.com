{
  "name": "personal-site",
  "description": "A website to show off some of my projects",
  "version": "1.0.0",
  "private": true,
  "author": "https://www.github.com/innellea",
  "license": "MIT",
  "keywords": [
    "nextjs",
    "starter",
    "typescript"
  ],
  "scripts": {
    "dev": "next",
    "debug": "cross-env NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx|html|css|scss)\" --write",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@chakra-ui/icons": "1.0.15",
    "@chakra-ui/react": "1.6.7",
    "@emotion/react": "11.4.1",
    "@emotion/styled": "11.3.0",
    "@mapbox/rehype-prism": "0.7.0",
    "@mdx-js/loader": "1.6.22",
    "@next/mdx": "11.1.1",
    "axios": "0.21.1",
    "cross-env": "7.0.3",
    "date-fns": "2.23.0",
    "firebase-admin": "9.11.1",
    "framer-motion": "4.1.17",
    "gray-matter": "4.0.3",
    "mdx-prism": "0.3.4",
    "next-mdx-remote": "3.0.4",
    "next-plausible": "2.1.1",
    "next-seo": "4.26.0",
    "next-transpile-modules": "8.0.0",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-awesome-reveal": "3.8.1",
    "react-dom": "17.0.2",
    "react-ga": "3.3.0",
    "react-hotkeys-hook": "3.4.0",
    "react-icons": "4.2.0",
    "react-palette": "1.0.2",
    "react-query": "3.21.1",
    "react-text-transition": "1.3.0",
    "reading-time": "1.4.0",
    "remark-autolink-headings": "7.0.1",
    "remark-code-titles": "0.1.2",
    "remark-slug": "7.0.0",
    "tslib": "2.3.1"
  },
  "devDependencies": {
    "@babel/core": "7.15.0",
    "@babel/preset-env": "7.15.0",
    "@babel/preset-react": "7.14.5",
    "@commitlint/cli": "13.1.0",
    "@commitlint/config-conventional": "13.1.0",
    "@firebase/app-types": "0.7.0",
    "@types/mdx-js__react": "1.5.4",
    "@types/node": "16.7.1",
    "@types/react": "17.0.19",
    "@types/react-dom": "17.0.9",
    "@typescript-eslint/eslint-plugin": "4.30.0",
    "@typescript-eslint/parser": "4.30.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.32.0",
    "eslint-config": "0.3.0",
    "eslint-config-next": "11.1.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-typescript": "2.4.0",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.4.1",
    "eslint-plugin-react": "7.25.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "7.0.2",
    "next": "11.1.1",
    "prettier": "2.3.2",
    "rehype": "12.0.0",
    "typescript": "4.4.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx next lint",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
